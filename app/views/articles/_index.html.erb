<div class="allwrapper">
  <div class="header-container">
    <div class="header-content">
      <div class="header-left">
        <div class="header-title">
          Qiita
        </div>
        <div class="search-content">
          <form class="search-form" action="#" method="get">
            <input class='input-box' placeholder='キーワードから探す' type="text" name="keyword" id="keyword" > 
            <button class="search-button">
              <%= image_tag "search.png", class:"search-icon" %>
            </button>
          </form>        
        </div>
      </div>
      <div class="header-right">
        <div class="regist-btn">
          <%= link_to "ユーザー登録", "#", class: 'regist-btn-text' %>
        </div>
        <div class="sign_in-btn">
          <%= link_to "ログイン", "#", class: 'sign_in-btn-text' %>
        </div>
      </div>       
    </div>
  </div>

  <div class="main-container">
    <div class="top-content">
    </div>
    <div class="under-content">
      <div class="sub-out-content">
        <div class="sub-in-content">
          <div class="sub-btn">
            <%= link_to "ホーム", "#", class:"sub-btn-text" %>
          </div>
          <div class="sub-btn">
            <%= link_to "プロフィール", "#", class:"sub-btn-text" %>
          </div>
          <div class="sub-btn">
            <%= link_to "お気に入り", "#", class:"sub-btn-text" %>
          </div>
          <div class="sub-btn">
            <%= link_to "チャット", "#", class:"sub-btn-text" %>
          </div>

        </div>
      </div>

      <div class="main-content">
      456
      </div>
    </div>
  </div>

  <div class="footer-container">
    <div class="footer-content">
        Copyright Qiita 2021.
    </div>
  </div>


</div>






